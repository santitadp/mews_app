<h1>Today's World News</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Summary</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entries.each do |entry| %>
      <tr>
        <td><%= entry.title %></td>
        <td><%= entry.summary %></td>
        <td><a href = "<%= entry.url %>">Link to Story</a></td>
      </tr>
    <% end %>
  </tbody>
</table>
